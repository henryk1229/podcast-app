<h2 class="users">New User<h1>
  <h3>

  <%= form_for @user do |f| %>
  <h3 class="users" ><%= f.label :first_name %>:</h3>
  <p><%= f.text_field :first_name %><br />

  <h3 class="users"><%= f.label :last_name %>:</h3>
  <p><%= f.text_field :last_name %><br />

  <h3 class="users"<%= f.label :username %>:</h3>
  <p><%= f.text_field :username %><br />

  <h3 class="users"<%= f.label :email %>:</h3>
  <p><%= f.email_field :email %><br />

  <h3 class="users"<%= f.label :password %>:</h3>
  <p><%= f.password_field :password %><br />

  <h4 class="users"> Tell us about yourself! </h3>
  <h3 class="users"<%= f.label :bio %>:</h3>
  <p><%= f.text_area :bio %> <br />

  <% if @user.errors.any? %>
    <div>
      <ul class="users">
      <% @user.errors.full_messages.each do |message| %>
        <li class="users"  ><%= message %></li>
      <% end %>
      </ul>
    </div>
  <%end%>

  <p><%= f.submit "Create Profile" %>
<% end %>
